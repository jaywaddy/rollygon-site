---
interface Props {
    color?: string;
    size?: string;
    stroke?: string;
}

const { color, size = "16", stroke="var(--stroke-width)" } = Astro.props;
---

<svg width={size} height={size} viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
<path d="M4 6L8 10L12 6" stroke={color} stroke-width={stroke} stroke-linecap="round" stroke-linejoin="round"/>
</svg>
