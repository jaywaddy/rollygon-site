---
interface Props {
    color?: string;
    size?: string;
    stroke?: string;
}

const { color, size = "16", stroke="var(--stroke-width)" } = Astro.props;
---

<svg width={size} height={size} viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
<path d="M4.66699 11.3334L11.3337 4.66669M11.3337 4.66669H4.66699M11.3337 4.66669V11.3334" stroke={color} stroke-width={stroke} stroke-linecap="round" stroke-linejoin="round"/>
</svg>
